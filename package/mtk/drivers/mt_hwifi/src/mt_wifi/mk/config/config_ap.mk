#Support SW-based zero wait DFS function
CONFIG_DFS_ADJ_BW_ZERO_WAIT=y
#endif /* DFS_ADJ_BW_ZERO_WAIT */

#ifdef DFS_SDB_SUPPORT
#Support selectable dual band(use dedicateRX to monitor inband radar)
CONFIG_DFS_SDB_SUPPORT=y
#endif /* DFS_SDB_SUPPORT */

#Support IGMP-Snooping function.
CONFIG_IGMP_SNOOP_SUPPORT=y

#Support Background Scan function
CONFIG_BACKGROUND_SCAN_SUPPORT=n

#ifdef WEPAUTO_OPEN_FIRST
#Support WEPAUTO mode try Open first then shared
