#ifdef MT7990
ifneq ($(findstring mt7990,$(CHIPSET)),)
#------ Chip HW/SW definitions ------#
CONFIG_MT_RF=y
CONFIG_MT_BBP=y
CONFIG_MT_MAC=y
CONFIG_MT_FMAC=n
CONFIG_MT_BMAC=y
CONFIG_MT_DMAC=y
CONFIG_INF_PCI=y
CONFIG_MAC_PCI=y
CONFIG_CUT_THROUGH=y
CONFIG_NEED_ROM_PATCH=y
CONFIG_NEED_FW_HEADER=n
#CONFIG_FWOWN_SUPPORT=y
CONFIG_MU_MIMO_SUPPORT=n
#Todo: Have build error when MIMO enable
CONFIG_FALCON_MURU_SUPPORT=y
CONFIG_HDR_TRANS_TX_SUPPORT=y
CONFIG_HDR_TRANS_RX_SUPPORT=y
CONFIG_HW_TX_AMSDU_SUPPORT=y
CONFIG_DOT11_VHT_SUPPORT=y
CONFIG_DOT11_HE_SUPPORT=y
CONFIG_DOT11_EHT_SUPPORT=y
CONFIG_RATE_ADAPT_AGBS_SUPPORT=y
CONFIG_RACTRL_FW_OFFLOAD_SUPPORT=y
CONFIG_RX_SCATTER=y
CONFIG_CTXD_MEM_CPY=y
CONFIG_CTXD_SCATTER_AND_GATHER=n
CONFIG_RX_CUT_THROUGH=n
CONFIG_UNIFIED_COMMAND=y
CONFIG_VOW_SUPPORT=y
CONFIG_CAL_FREE_IC_SUPPORT=y
CONFIG_PRE_CAL_SUPPORT=y
CONFIG_SWACI_MECHANISM_SUPPORT=y

WFLAGS += -DSOFT_BIP_GMAC
WFLAGS += -DMT7990 -DA_BAND_SUPPORT
WFLAGS += -DMAC_INIT_OFFLOAD
WFLAGS += -DPKT_BUDGET_CTRL_SUPPORT
WFLAGS += -DSTA_LP_PHASE_2_SUPPORT -DCONFIG_PM_BIT_HW_MODE
WFLAGS += -DDBG_AMSDU
WFLAGS += -DDOT11V_MBSSID_SUPPORT -DPEAK_ENHANCE -DCFG_JUMBO_SUPPORT -DADJUST_TXOP_NEW_ARCH
WFLAGS += -DCONFIG_3_WIRE_SUPPORT
WFLAGS += -DCFG_WIFI_MT7990_BAND_NUM=3
ifeq ($(CONFIG_FPGA_MODE),y)
CONFIG_NEED_ROM_PATCH=n
WFLAGS += -DMT7990_FPGA
WFLAGS += -DCONFIG_FPGA_MODE
endif
ifeq ($(CONFIG_PRE_CAL_SUPPORT),y)
WFLAGS += -DPRE_CAL_MT7990_SUPPORT
endif
ifeq ($(CONFIG_IGMP_SNOOP_SUPPORT),y)
WFLAGS += -DIGMP_SNOOPING_NON_OFFLOAD -DIGMP_SNOOPING_DENY_LIST
endif
ifeq ($(CONFIG_CSI_SUPPORT),y)
WFLAGS += -DCFG_SUPPORT_CSI
endif
endif
#endif /* End of MT7990 */

