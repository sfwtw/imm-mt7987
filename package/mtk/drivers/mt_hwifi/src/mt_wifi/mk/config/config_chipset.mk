################################################################
# Chipset Compiler Flag
################################################################
#------ Chip Feature ------#
ifeq ($(CONFIG_MT_RF),y)
WFLAGS += -DMT_RF
endif
ifeq ($(CONFIG_MT_BBP),y)
WFLAGS += -DMT_BBP
endif
ifeq ($(CONFIG_MT_MAC),y)
WFLAGS += -DMT_MAC
endif
ifeq ($(CONFIG_MT_DMAC),y)
WFLAGS += -DMT_DMAC
endif
ifeq ($(CONFIG_MT_FMAC),y)
WFLAGS += -DMT_FMAC
endif
ifeq ($(CONFIG_MT_BMAC),y)
WFLAGS += -DMT_BMAC
endif
ifeq ($(CONFIG_INF_PCI),y)
WFLAGS += -DRTMP_PCI_SUPPORT
endif
ifeq ($(CONFIG_MAC_PCI),y)
WFLAGS += -DRTMP_MAC_PCI
endif
ifeq ($(CONFIG_INF_RBUS),y)
WFLAGS += -DRTMP_RBUS_SUPPORT
endif
ifeq ($(CONFIG_INF_USB),y)
WFLAGS += -DRTMP_USB_SUPPORT
endif
ifeq ($(CONFIG_MAC_USB),y)
WFLAGS += -DRTMP_MAC_USB
endif
ifeq ($(CONFIG_CUT_THROUGH),y)
WFLAGS += -DCUT_THROUGH -DCUT_THROGH_DBG
ifeq ($(CONFIG_CTXD_MEM_CPY),y)
WFLAGS += -DCTXD_MEM_CPY -DCONFIG_TX_DELAY
endif
ifeq ($(CONFIG_CTXD_SCATTER_AND_GATHER),y)
WFLAGS += -DCTXD_SCATTER_AND_GATHER -DCONFIG_TX_DELAY
endif
endif
ifeq ($(CONFIG_RX_CUT_THROUGH),y)
WFLAGS += -DRX_CUT_THROUGH
endif
ifeq ($(CONFIG_CUT_THROUGH_DBG),y)
WFLAGS += -DCUT_THROUGH_DBG
endif
ifeq ($(CONFIG_NEED_ROM_PATCH), y)
WFLAGS += -DNEED_ROM_PATCH
endif
ifeq ($(CONFIG_NEED_FW_HEADER), y)
WFLAGS += -DNEED_FW_HEADER
endif
ifeq ($(CONFIG_FWOWN_SUPPORT),y)
WFLAGS += -DCONFIG_FWOWN_SUPPORT
endif
ifeq ($(CONFIG_FALCON_MURU_SUPPORT),y)
WFLAGS += -DCFG_SUPPORT_FALCON_MURU
endif
ifeq ($(CONFIG_RX_SCATTER),y)
WFLAGS += -DRX_SCATTER
endif
ifeq ($(CONFIG_HDR_TRANS_TX_SUPPORT),y)
WFLAGS += -DHDR_TRANS_TX_SUPPORT
endif
ifeq ($(CONFIG_HDR_TRANS_RX_SUPPORT),y)
WFLAGS += -DHDR_TRANS_RX_SUPPORT
endif
ifeq ($(CONFIG_HW_TX_AMSDU_SUPPORT),y)
WFLAGS += -DHW_TX_AMSDU_SUPPORT
endif
ifeq ($(CONFIG_UNIFIED_COMMAND), y)
WFLAGS += -DWIFI_UNIFIED_COMMAND
endif
ifeq ($(CONFIG_VOW_SUPPORT), y)
WFLAGS += -DVOW_SUPPORT
endif
ifeq ($(CONFIG_HIGH_PRIO_QUEUE_SUPPORT),y)
WFLAGS += -DHIGH_PRIO_QUEUE_SUPPORT
endif
